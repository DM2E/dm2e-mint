        <element ref="ore:Aggregation" maxOccurs="unbounded" minOccurs="1"/>
        <element name="Agent" maxOccurs="unbounded" minOccurs="0" type="edm:AgentType" />
      	<element ref="foaf:Person" maxOccurs="unbounded" minOccurs="0" />
      	<element ref="foaf:Organisation" maxOccurs="unbounded" minOccurs="0"/>
     </sequence>
   </complexType>

</schema>
